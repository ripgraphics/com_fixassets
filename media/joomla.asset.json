                "defer": true
            }
        },
        {
            "name": "com_fixassets.admin-css",
            "type": "style",
            "uri": "com_fixassets/css/admin.css",
            "dependencies": []
        },
        {
            "name": "com_fixassets.admin-js",
            "type": "script",
            "uri": "com_fixassets/js/admin.js",
            "dependencies": ["core"],
            "attributes": {
                "defer": true
            }
        }
    ]
